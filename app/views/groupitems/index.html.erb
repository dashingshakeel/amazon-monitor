
<h1>Products under Monitor</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="row">Name</th>
      <th scope="row">Url</th>
      <th scope="row"></th>
    </tr>
  </thead>

  <tbody>
    <% @groupitems.each do |item| %>

      <tr>
        
        <td><%= item.name %></td>
        <td><%= item.url %></td>
        <td> <%= link_to "View Log", groupitem_path(item) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
